{
  "name": "portal",
  "author": "",
  "homepage": "/",
  "version": "0.0.0",
  "scripts": {
    "analyze": "webpack-bundle-analyzer ./.next/stats.json",
    "build": "next build",
    "dev": "next",
    "lint": "eslint src/**/*.{ts,tsx} --ignore-pattern src/**/*.test.* -c .eslintrc.js  --fix",
    "serve": "next start -p 80",
    "start": "npm run dev",
    "test": "jest --config=jest-config.js --collectCoverage --detectOpenHandles --forceExit",
    "type-check": "tsc"
  },
  "dependencies": {
    "@go1d/go1d": "^0.9.37",
    "@go1d/go1d-exchange": "^0.2.43",
    "@go1d/mine": "^0.3.21",
    "axios": "^0.19.0",
    "debug": "^4.1.1",
    "dotenv": "^8.1.0",
    "emotion": "^9.2.12",
    "emotion-server": "^9.2.12",
    "lodash-webpack-plugin": "^0.11.5",
    "next": "^9.1.0",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-intl": "^2.9.0",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "stats-webpack-plugin": "^0.7.0",
    "universal-cookie": "^4.0.2",
    "webpack": "^4.41.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.8",
    "@types/react": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.2",
    "axios-mock-adapter": "^1.17.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-imports": "^2.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-only-warn": "^1.0.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.15.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "1.3.1",
    "jest": "^24.9.0",
    "jest-environment-jsdom-fourteen": "^0.1.0",
    "lint-staged": "8.1.5",
    "prettier": "^1.18.2",
    "prettier-package-json": "2.1.0",
    "redux-devtools-extension": "^2.13.8",
    "typescript": "^3.6.3",
    "webpack-bundle-analyzer": "^3.5.2"
  },
  "keywords": [],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run type-check && lint-staged"
    }
  },
  "lint-staged": {
    "[*.js,*.jsx,*.ts,*.tsx]": [
      "prettier --write",
      "git add"
    ],
    "[*.json, !package.json]": [
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ]
  },
  "pre-push": "lint"
}
